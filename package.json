  "scripts": {
    "test": "npm run eslint",
    "eslint": "eslint --ext .js,.ts src/",
    "dev": "tsc --watch",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "start": "ts-node ./src/service/service.ts --server 3000",
